# Reload
alias reload="source ~/.bashrc"

# Open
alias balias="code /home/alexanderolsson/.bash_aliases"

# Activate
alias venv="source /mnt/c/Users/AlexanderOlsson/repos/.venv/bin/activate"

# CD
